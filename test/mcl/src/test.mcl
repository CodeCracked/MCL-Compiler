namespace test
{
    listener mcl:reload()
    {
        float a = 4.5;
        float b = 2 * a;
        int i = (int)b;
        native ''tellraw @a %{i.text_component}'';
    }

    void print_double(int i)
    {
        int double = i * 2;
        native "tellraw @a %{double.text_component}";
    }
